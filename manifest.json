{
    "name": "wordwa11",
    "author": "pxtrez",
    "description": "Works for private quizzes, also on 'play' and 'resource' modes || Made by pxtrez || Discord: pxtrez#0001.",
    "version": "3.0",
    "background": {
        "content_scripts": [{
            "matches": [
                "*://*.wordwall.net*"
            ],
            "scripts": ["./src/script.js"]
        }]
    },
    "browser_action": {
        "default_icon": "./src/img/wordwa11.png",
        "default_popup": "./src/popup/popup.html",
        "default_title": "wordwa11"
    },
    "icons": {
        "128": "./src/img/wordwa11.png"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "activeTab"
    ],
    "manifest_version": 2
}